<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>SuiViDesDeamandesCasCNPS</display-name>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<servlet-class>controller.demandeur_controller.Register</servlet-class>
  	
  	<init-param>
  		<param-name>Auteur</param-name>
  		<param-value>Honore Remi</param-value>
  	</init-param>
  	
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
   <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>login.Login</servlet-class>
  	
  	<init-param>
  		<param-name>Auteur</param-name>
  		<param-value>Honore Remi</param-value>
  	</init-param>
  	
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/index</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
</web-app>